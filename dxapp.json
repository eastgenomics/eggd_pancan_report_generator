{
  "name": "eggd_pancan_report_generator",
  "title": "eggd_pancan_report_generator",
  "summary": "eggd_pancan_report_generator",
  "version": "1.0.0",
  "inputSpec": [
    {
      "name": "bam",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.bam"
      ],
      "help": ""
    },
    {
      "name": "rnaseqc_metrics",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.metrics.tsv"
      ],
      "help": ""
    },
    {
      "name": "rnaseqc_coverage",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.coverage.hgnc.tsv"
      ],
      "help": ""
    },
    {
      "name": "rnaseqc_exon",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.exon_cv.tsv"
      ],
      "help": ""
    },
    {
      "name": "fusioninspector_abridged",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.coding_effect"
      ],
      "help": ""
    },
    {
      "name": "capture_bed",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.bed"
      ],
      "help": ""
    },
    {
      "name": "ensdb_gtf",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.sqlite"
      ],
      "help": ""
    },
    {
      "name": "chimerviz_docker",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.tar.gz"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "html_report",
      "class": "file",
      "patterns": [
        "*.html"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 2
      }
    },
    "interpreter": "bash",
    "file": "src/eggd_pancan_report_generator.sh",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "network": [
      "*"
    ],
    "project": "CONTRIBUTE"
  },
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd2_v2_x16"
        }
      }
    }
  }
}
